<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #06f3bd;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.MainView">
   <children>
      <ImageView fitHeight="118.0" fitWidth="118.0" layoutX="241.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../png/pacmanRight.gif" />
         </image>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </ImageView>
      <Button layoutX="255.0" layoutY="145.0" mnemonicParsing="false" onMouseClicked="#play" prefHeight="37.0" prefWidth="89.0" style="-fx-background-color: Yellow;" text="Play">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="224.0" layoutY="194.0" mnemonicParsing="false" onMouseClicked="#changePassword" prefHeight="40.0" prefWidth="151.0" style="-fx-background-color: Yellow;" text="Change password">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="233.0" layoutY="245.0" mnemonicParsing="false" onMouseClicked="#deleteAccount" prefHeight="40.0" prefWidth="134.0" style="-fx-background-color: Yellow;" text="Delete account">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="262.0" layoutY="297.0" mnemonicParsing="false" onMouseClicked="#logout" prefHeight="40.0" prefWidth="71.0" style="-fx-background-color: Yellow;" text="Logout">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="514.0" layoutY="358.0" mnemonicParsing="false" onMouseClicked="#exit" prefHeight="18.0" prefWidth="71.0" style="-fx-background-color: Red;" text="Exit">
         <font>
            <Font size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="mute" layoutX="23.0" layoutY="358.0" mnemonicParsing="false" onMouseClicked="#mute" prefHeight="27.0" prefWidth="71.0" style="-fx-background-color: green;" text="Mute" />
      <Button layoutX="117.0" layoutY="358.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="90.0" style="-fx-background-color: Green;" text="Change Song" onMouseClicked="#changeMedia" />
   </children>
</AnchorPane>
